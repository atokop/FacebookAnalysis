{
  "manifest_version": 2,

  "name": "Facebook Analyser",
  "description": "This extension analyses facebook posts.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icon_24.png",
    "default_popup": "popup.html"
  },
  "background": {
"scripts": ["background.js", "jquery-2.0.3.min.js"]
},
"permissions": [ "tabs", "http://*/*", "https://*/*"]
}